{
    "name": "julianobailao/cross-docking",
    "type": "library",
    "description": "Uma maneira fácil de obter informações dos cross dockings das principais distribuidoras do brasil.",
    "keywords": [
        "flyingluscas",
        "correios-php",
        "correios",
        "frete",
        "calculo",
        "rastreio",
        "cep"
    ],
    "homepage": "https://github.com/oemshop/cross-docking",
    "license": "MIT",
    "authors": [
        {
            "name": "Juliano Bailao",
            "email": "julianobailao@gmail.com",
            "homepage": "https://github.com/julianobailao",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "~0.3",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "JulianoBailao\\CrossDocking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulianoBailao\\CrossDocking\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
